{
  "name": "silexfriends/di",
  "description": "A simple dependency injection builder to Silex",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Thiago Paes",
      "email": "mrprompt@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "SilexFriends\\Di\\": "src/Di/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SilexFriends\\Tests\\Di\\": "tests/Di/",
      "SilexFriends\\Tests\\Resources\\": "tests/Resources/"
    }
  },
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "silex/silex": "1.*@dev",
    "deralex/yaml-config-service-provider": "~1.0"
  }
}
